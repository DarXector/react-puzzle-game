{
  "name": "express-react-redux-starter",
  "version": "1.1.0",
  "description": "Starter for Express, React, Redux, SCSS applications",
  "scripts": {
    "dev": "webpack-dev-server --watch --colors",
    "build": "rimraf dist && webpack -p",
    "start": "SET NODE_ENV=production PORT=8080 pm2 start ./server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src test webpack"
  },
  "keywords": [
    "ExpressJS",
    "ReactJS",
    "Redux",
    "React hot loader",
    "React Router",
    "SCSS",
    "Webpack Devevelopment configuration",
    "Webpack Production configuration",
    "Airbnb Eslint",
    "pm2",
    "mocha",
    "chai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DimitriMikadze/express-react-redux-starter"
  },
  "author": "Dimitri Mikadze",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.16.1",
    "jquery": "^3.2.1",
    "jsdom": "^10.1.0",
    "mocha": "^3.3.0",
    "node-sass": "^4.5.2",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
